<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MovieArchive.Home"
             xmlns:pages="clr-namespace:MovieArchive">
    <FlyoutPage.Flyout>
        <pages:HomeMaster x:Name="MasterPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
              <pages:Dashboard />
            </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>